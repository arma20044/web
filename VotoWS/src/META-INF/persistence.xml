<?xml version="1.0" encoding="UTF-8"?>
<persistence
  xmlns="http://java.sun.com/xml/ns/persistence"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
                                 http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
  version="1.0">
  <persistence-unit
    name="simple-jpa"
    transaction-type="RESOURCE_LOCAL">
   <class>py.edu.ucsa.voto.ws.schema.beans.ConsultarRequest</class>
   <class>py.edu.ucsa.voto.ws.schema.beans.ConsultarResponse</class>
   <class>py.edu.ucsa.voto.ws.schema.beans.VotarRequest</class>
   <class>py.edu.ucsa.voto.ws.schema.beans.VotarResponse</class>
   <class>py.edu.ucsa.voto.ws.schema.beans.AutenticarRequest</class>
   <class>py.edu.ucsa.voto.ws.schema.beans.AutenticarResponse</class>
   <class>py.edu.ucsa.voto.entity.Genero</class>
   <class>py.edu.ucsa.voto.entity.Users</class>
   <class>py.edu.ucsa.voto.ws.schema.beans.QueryGenericoRequest</class>
   <class>py.edu.ucsa.voto.ws.schema.beans.QueryGenericoResponse</class>
    <exclude-unlisted-classes />
    <properties>
      <property
        name="hibernate.dialect"
        value="org.hibernate.dialect.PostgreSQLDialect" />
      <property
        name="hibernate.show_sql"
        value="true" />
      <property
        name="hibernate.hbm2ddl.auto"
        value="update" />
    </properties>
  </persistence-unit>
</persistence>